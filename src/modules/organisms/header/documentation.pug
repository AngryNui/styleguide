extends ../../../styleguide/styleguide.layout.pug

include ../../molecules/searchbar/searchbar.mixin.pug


block main
  +sg-header({ title: 'Header' })
  +sg-tabs({
    linkList: [
      { title: 'Documentation', url: '/modules/organisms/header/documentation.html', active: true },
      { title: 'Code', url: '/modules/organisms/header/code.html' },
      { title: 'Example', url: '/modules/organisms/header/example.html' }
    ],
  })

  -
    var usedModules = self.usedModules || [];

  each module in usedModules
    p= module

  +sg-markdown('src/modules/organisms/header/README.md')
